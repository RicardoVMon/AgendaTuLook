@model HorariosModel
<form asp-controller="Horarios" asp-action="RegistrarHorario">
    <div class="row">

        <h2 class="text-center">Crear Horario</h2>
        <div class="form-group col-md-12 mb-4">
            <input type="datetime" required class="form-control input-lg" asp-for="StartDate" placeholder="fecha de inicio">
        </div>
        <div class="form-group col-md-12 mb-4">
            <input type="datetime" class="form-control  input-lg" asp-for="EndDate" placeholder="Fecha de finalización">

        </div>
            <div class="col-md-12">

                <button type="submit" class="btn btn-primary btn-pill mb-4">RegistrarHorario</button>


            </div>
        
    </div>
</form>